{
    "name": "enrj/phumbor-bundle",
    "type": "symfony-bundle",
    "description": "A bridge for symfony with the phumbor client from 99designs",
    "keywords": ["phumbor", "thumbor", "Bundle", "Helper", "Twig", "Symfony"],
    "homepage": "https://github.com/jbouzekri/PhumborBundle",
    "license": "MIT",
    "require": {
        "webfactory/phumbor": "^1.2"
    },
    "require-dev": {
        "symfony/config": "~2.4",
        "symfony/http-kernel": "~2.4",
        "symfony/dependency-injection": "~2.4",
        "twig/extensions": "~1.0",
        "phpunit/phpunit": "~4.8|~5.7"
    },
    "autoload": {
        "psr-0": { "Jb\\Bundle\\PhumborBundle": "" }
    },
    "target-dir": "Jb/Bundle/PhumborBundle"
}
